{
  "name": "rosestclient",
  "main": "dist/server.js",
  "version": "0.0.0",
  "scripts": {
    "start": "node --harmony ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "webpack --bail --colors --display-error-details --config configs/webpack.client.js",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\""
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-hmr": "^0.2.0",
    "angular-input-masks": "^2.1.1",
    "angular-messages": "^1.5.0",
    "angular-money-directive": "^1.2.4",
    "angular-resource": "^1.5.0",
    "angular-sanitize": "^1.5.0",
    "angular-scroll": "^1.0.0",
    "angular-socialshare": "^0.2.4-beta",
    "angular-stripe-checkout": "^4.0.1",
    "angular-ui-bootstrap": "^1.1.2",
    "angular-ui-notification": "^0.1.0",
    "angular-ui-router": "^0.2.18",
    "angular-ui-utils": "^0.1.1",
    "angular-utils-disqus": "https://github.com/michaelbromley/angularUtils-disqus.git",
    "angular-utils-pagination": "^0.10.0",
    "angulartics": "^1.0.3",
    "angulartics-google-analytics": "^0.1.4",
    "axios": "^0.9.1",
    "babel-polyfill": "^6.3.14",
    "ejs": "^2.4.1",
    "fs-extra": "^0.26.7",
    "koa": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-ejs": "^3.0.0",
    "koa-logging": "^0.1.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "koa-useragent": "^0.1.2",
    "ng-annotate-loader": "^0.1.0",
    "ng-currency-mask": "https://github.com/vtex/ng-currency-mask.git",
    "ng-forward": "0.0.1-alpha.12",
    "node-schedule": "^1.1.0",
    "resolve-url-loader": "^1.4.3",
    "schedule": "^0.1.0",

  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "app-root-path": "^1.0.0",
    "autoprefixer": "^6.3.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.4.3",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.14.0",
    "jasmine-core": "^2.4.1",
    "jasmine-expect": "^2.0.2",
    "json-loader": "^0.5.4",
